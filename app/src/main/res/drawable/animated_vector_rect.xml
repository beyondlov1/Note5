<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/vector_rect">
    <target
        android:animation="@animator/trim_stroke_longer"
        android:name="rect_red_top"/>
    <target
        android:animation="@animator/trim_stroke_longer"
        android:name="rect_red_right"/>
    <target
        android:animation="@animator/trim_stroke_longer"
        android:name="rect_red_bottom"/>
    <target
        android:animation="@animator/trim_stroke_longer"
        android:name="rect_red_left"/>
    <target
        android:animation="@animator/trim_stroke_darker"
        android:name="rect_red_top"/>
    <target
        android:animation="@animator/trim_stroke_darker"
        android:name="rect_red_right"/>
    <target
        android:animation="@animator/trim_stroke_darker"
        android:name="rect_red_bottom"/>
    <target
        android:animation="@animator/trim_stroke_darker"
        android:name="rect_red_left"/>

    <target
        android:animation="@animator/trim_stroke_lighter"
        android:name="rect_gray"/>
</animated-vector>